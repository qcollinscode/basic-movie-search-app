<!--
  include partials/header
-->
<% include partials/header %>

<!-- Section -->
<section>

  <!-- Container -->
  <div align="center">

    <!-- Title -->
    <h1>Movies</h1>

    <!-- Movie List -->
    <ul>
      <% data["Search"].forEach(function (movie) { %>
        <li> <strong><%= movie["Title"] %></strong> - <%= movie["Year"] %> </li>
      <% }) %>
    </ul>

    <!-- Back button -->
    <a href="/">Search Again!</a>
  </div>
</section>

<!--
  include partials/footer.ejs
-->
<% include partials/footer %>
